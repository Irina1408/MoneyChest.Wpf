<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MoneyChest.Languages"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
     
    <!-- Buttons -->
    <system:String x:Key="lOk">Ok</system:String>
    <system:String x:Key="lCancel">Cancel</system:String>

    <!-- Login window -->
    <system:String x:Key="lName">Name</system:String>
    <system:String x:Key="lPassword">Password</system:String>

    <!-- Main window -->
    <system:String x:Key="lDashboard">Dashboard</system:String>
    <system:String x:Key="lTransactions">Transactions</system:String>
    <system:String x:Key="lCalendar">Calendar</system:String>
    <system:String x:Key="lStorages">Accounts</system:String>
    <system:String x:Key="lPlanning">Planning</system:String>
    <system:String x:Key="lDebts">Debts</system:String>
    <system:String x:Key="lReports">Reports</system:String>
    <system:String x:Key="lCurrencies">Currencies</system:String>
    <system:String x:Key="lCategories">Categories</system:String>
    <system:String x:Key="lSettings">Settings</system:String>

</ResourceDictionary>